<Window x:Class="ProgramPraca.UserManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgramPraca"
        mc:Ignorable="d"
        Title="UserManager" Height="180" Width="300">
    <Grid>
        <Button x:Name="but1" Content="Dodaj Użytkownika" HorizontalAlignment="Left" Margin="50,28,0,0" VerticalAlignment="Top" Panel.ZIndex="6" Background="White" BorderBrush="#FFAEAEAE" Width="200" Height="34" UseLayoutRounding="True" Foreground="#FF87BC5E" ClickMode="Release" FontSize="16" Click="AddUser"/>
        <Button x:Name="but2" Content="Usuń Użytkownika" HorizontalAlignment="Left" Margin="50,67,0,0" VerticalAlignment="Top" Panel.ZIndex="6" Background="White" BorderBrush="#FFAEAEAE" Width="200" Height="34" UseLayoutRounding="True" Foreground="#FF87BC5E" ClickMode="Release" FontSize="16" Click="DellUser"/>

    </Grid>
</Window>
